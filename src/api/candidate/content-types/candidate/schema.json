  },
  "pluginOptions": {},
  "attributes": {
    "candidateId": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "contactNumber": {
      "type": "biginteger",
      "unique": true,
      "required": true
    },
    "resume": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "files"
      ]
    },
    "network": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::network.network",
      "inversedBy": "candidate"
    },
    "candidatePhoto": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
